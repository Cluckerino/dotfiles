# Local/private config goes in the include
[core]
        pager = cat
        excludesfile = ~/.gitexcludes
[include]
        path = ~/.gitconfig.local
[hub]
        protocol = https
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[help]
        autocorrect = 1
[pull]
        rebase = true
[init]
	defaultBranch = main
